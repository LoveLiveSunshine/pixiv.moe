{
  "name": "pixiv.moe",
  "version": "1.2.6",
  "description": "Source code of https://pixiv.moe",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy": "copyfiles -f ./src/index.html ./dist",
    "predist": "npm run clean && npm run copy",
    "dist": "cross-env NODE_ENV=production webpack --config build/webpack.config.js",
    "postdist": "copyfiles -f ./src/icons/*.ico ./src/icons/*.png ./dist/assets && copyfiles -f ./src/manifest.json ./dist",
    "archive": "cd dist && tar -czvf dist.tar.gz * --exclude=*.gz",
    "lint": "eslint --max-warnings 0 ./src",
    "lint:fix": "eslint --fix ./src",
    "posttest": "npm run lint",
    "start": "cross-env NODE_ENV=development webpack-dev-server --progress --config build/webpack.config.js",
    "test": "cross-env NODE_ENV=test karma start",
    "test:watch": "cross-env NODE_ENV=test karma start --autoWatch=true --singleRun=false",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "check": "yarn upgrade-interactive"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LoveLiveSunshine/pixiv.moe.git"
  },
  "keywords": [
    "pixiv",
    "lovelive"
  ],
  "author": "kokororin",
  "license": "MIT",
  "homepage": "https://github.com/LoveLiveSunshine/pixiv.moe#readme",
  "bugs": {
    "url": "https://github.com/LoveLiveSunshine/pixiv.moe/issues"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.26.0",
