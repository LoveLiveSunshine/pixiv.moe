{
  "name": "pixiv.moe",
  "version": "1.5.0",
  "description": "A pinterest-style layout site, shows illusts on [pixiv.net](http://pixiv.net) order by popularity. Written with React.",
  "scripts": {
    "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('You must use Yarn to install, not NPM')\"",
    "clean": "rimraf dist/*",
    "predist": "npm run clean",
    "dist": "cross-env NODE_ENV=production webpack --config build/webpack.config.js",
    "postdist": "copyfiles -f ./src/icons/*.ico ./src/icons/*.png ./dist/assets && copyfiles -f ./src/manifest.json ./dist",
    "lint": "eslint src test server",
    "lint:fix": "eslint --fix src test server",
    "posttest": "npm run lint",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.config.js",
    "test": "cross-env NODE_ENV=test karma start",
    "test:watch": "cross-env NODE_ENV=test karma start --autoWatch=true --singleRun=false",
    "deploy": "gh-pages -d dist",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "backend:dev": "cross-env NODE_ENV=development nodemon --watch ./server ./server/server.js -- ",
    "backend:prod": "cross-env NODE_ENV=production node ./server/server.js",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LoveLiveSunshine/pixiv.moe.git"
  },
  "keywords": [
    "pixiv",
    "lovelive"
  ],
  "author": "kokororin",
  "license": "MIT",
  "homepage": "https://github.com/LoveLiveSunshine/pixiv.moe#readme",
  "bugs": {
    "url": "https://github.com/LoveLiveSunshine/pixiv.moe/issues"
  },
  "devDependencies": {
