    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.8",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^4.1.1",
